{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "noUncheckedIndexedAccess": false
  }
}
