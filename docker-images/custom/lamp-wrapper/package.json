{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:1-rimraf": "rimraf dist",
    "build:2-mkdir": "mkdirp dist",
    "build:3-rollup": "rollup --silent --config rollup.config.js",
    "build:4-terser": "terser --config-file terser.json dist/main.js --output dist/main.js",
    "lint": "tsc --noEmit --skipLibCheck",
    "postinstall": "patch-package --patch-dir patches",
    "postinstall:patch": "patch-package --patch-dir patches",
    "start": "nodemon --config nodemon.json"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "express-json-validator-middleware": "^3.0.1",
    "express-prom-bundle": "^8.0.0",
    "punycode": "^2.3.1",
    "tslib": "^2.8.1",
    "winston": "^3.17.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.9.0",
    "@types/yargs": "^17.0.33",
    "glob-exec": "^0.1.1",
    "mkdirp": "^3.0.1",
    "modclean": "^3.0.0-beta.1",
    "node-prune": "^1.0.2",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.25.0",
    "terser": "^5.36.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
